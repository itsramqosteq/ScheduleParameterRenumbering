<UserControl x:Class="ScheduleParameterRenumbering.SquareImportExcelUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ScheduleParameterRenumbering"
             xmlns:materialDesign="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             mc:Ignorable="d" 
             >
    <Grid>

        <local:SquareButtonUserControl x:Name="btnImport" Icon="Import" Margin="0,10,0,0"  
                                   Label="Import Excel" 
                                     BtnClick="BtnImport_Click"></local:SquareButtonUserControl>
    </Grid>
</UserControl>
